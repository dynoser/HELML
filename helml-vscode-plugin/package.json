{
  "name": "helml",
  "displayName": "helml",
  "description": "Plugin to encode/decode HELML data",
  "version": "0.0.9",
  "publisher": "dynoser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dynoser/HELML/tree/master/helml-vscode-plugin"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "helml",
        "extensions": [
          ".helml",
          ".hml"
        ]
      }
    ],
    "grammars": [
      {
        "language": "helml",
        "scopeName": "source.helml",
        "path": "./syntaxes/helml.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "helml.toJSON",
        "title": "HELML toJSON"
      },
      {
        "command": "helml.fromJSON",
        "title": "HELML fromJSON"
      },
      {
        "command": "helml.fromJSONDoc",
        "title": "HELML fromJSON to NewDoc"
      },
      {
        "command": "helml.toPHP",
        "title": "HELML toPHP"
      },
      {
        "command": "helml.toPython",
        "title": "HELML toPython"
      }
    ],
    "configuration": {
      "title": "HELML Config",
      "properties": {
        "helml.enableident": {
          "type": "boolean",
          "default": true,
          "description": "HELML encoder adds space indentation"
        },
        "helml.enablebones": {
          "type": "boolean",
          "default": true,
          "description": "HELML encoder adds next-num-keys instead of numbers"
        },
        "helml.enableuplines": {
          "type": "boolean",
          "default": true,
          "description": "HELML encoder adds empty line before create-array-keys"
        },
        "helml.enablehashsym": {
          "type": "boolean",
          "default": true,
          "description": "HELML encoder adds #-symbol after nested-blocks"
        }
      }
    }
  },
  "scripts": {},
  "devDependencies": {
    "@types/node": "^14.18.42",
    "@types/vscode": "^1.65.0"
  }
}
